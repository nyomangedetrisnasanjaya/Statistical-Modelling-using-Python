    "location_price = df.pivot_table(values='price',\n",
    "                       index=['name'],\n",
    "                       columns=['source','destination'],\n",
    "                       aggfunc=np.mean,\n",
    "                       fill_value=0)\n",
    "location_price.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "2809f48d",
